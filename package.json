{
  "name": "incident-reporter",
  "version": "1.0.0",
  "description": "Incident reporting api for insurance clients",
  "main": "index.js",
  "author": "paul osei kofi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts,.json",
    "start": "pm2-runtime ecosystem.config.js --json --only api",
    "prototype": "prisma db push --accept-data-loss",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "schema:format": "prisma format && yarn generate",
    "reset:db": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "dev": "set DEBUG=incident-reporter:* && nodemon ./src/index.ts",
    "migrate:dev": "prisma migrate dev",
    "test": "set DEBUG=incident-reporter:* && jest"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": ">=28.0.0 <29.0.0-0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "husky": "^8.0.1",
    "jest": ">=28.0.0 <29.0.0-0",
    "nodemon": "^2.0.19",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-rate-limit": "^6.6.0",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0"
  }
}
